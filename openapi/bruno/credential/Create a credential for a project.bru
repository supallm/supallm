meta {
  name: Create a credential for a project
  type: http
  seq: 2
}

post {
  url: {{baseURL}}/projects/{{projectId}}/credentials
  body: json
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
    "name": "",
    "provider": "",
    "apiKey": ""
  }
}

tests {
  let cl = res.getHeader("content-location")
  let part = cl.split("/")
  let id = part[4]
  bru.setVar("credentialId", id)
}
